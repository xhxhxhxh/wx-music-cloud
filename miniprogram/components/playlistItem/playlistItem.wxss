/* components/playlistItem/playlistItem.wxss */
.playlist-item {
  width: 220rpx;
  position: relative;
  padding-bottom: 20rpx;
}
.playlist-img {
  width: 100%;
  height: 220rpx;
  border-radius: 6rpx;
}
.palylist-count {
  position: absolute;
  top: 0;
  right: 6rpx;
  color: #fff;
  font-size: 24rpx;
  padding-left: 30rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 24rpx;  
  background-position: left center;
}
.palylist-name {
  font-size: 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 1.3;
  padding: 2px 0 0 6px;
}